#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/C:/Users/trust/Desktop/blogs/node_modules/.store/he@0.5.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/C:/Users/trust/Desktop/blogs/node_modules/.store/he@0.5.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../he@0.5.0/node_modules/he/bin/he" "$@"
else
  exec node  "$basedir/../../../../../he@0.5.0/node_modules/he/bin/he" "$@"
fi
